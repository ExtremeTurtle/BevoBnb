@model IEnumerable<String>

<h1>Error</h1>

<p>Oops - something went wrong. Are you sure you are signed into the correct account?</p>

@if (Model != null)
{
    foreach (String message in Model)
    {
        <p><span class="text-danger">@message</span></p>
    }
}

<a asp-action="Index" asp-controller="Home" class="btn btn-success">Take me home!</a>
<br />
<form asp-action="SwitchAccount" asp-controller="Account" method="post">
    <button type="submit" class="btn btn-success">Switch Account</button>
</form>